{
  "expo": {
    "name": "Edplore Sales Catalogue",
    "slug": "edplore-sales-catalogue",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "android": {
      "package": "com.praphul09.edploresalescatalogue",
      "newArchEnabled": true
    }
  }
}
