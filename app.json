{
  "expo": {
    "name": "Edplore Sales Catalogue",
    "slug": "edplore-sales-catalogue",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*",
      "assets/pdfjs/*"
    ],
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "android": {
      "package": "com.praphul09.edploresalescatalogue",
      "newArchEnabled": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2714fd4e-f025-470b-b4be-d34ee2859ee2"
      }
    }
  }
}
